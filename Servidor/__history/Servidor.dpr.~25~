program Servidor;
{$APPTYPE GUI}

uses
  Vcl.Forms,
  Web.WebReq,
  IdHTTPWebBrokerBridge,
  uView.Principal in 'uView.Principal.pas' {Principal},
  ServerMethodsUnit1 in 'ServerMethodsUnit1.pas' {ServerMethods1: TDSServerModule},
  ServerContainerUnit1 in 'ServerContainerUnit1.pas' {ServerContainer1: TDataModule},
  WebModuleUnit1 in 'WebModuleUnit1.pas' {WebModule1: TWebModule},
  DAO.DMConexao in 'Model\DAO.DMConexao.pas' {DMConexao: TDataModule},
  uJsonDataSetHelper in '..\Repositorio\uJsonDataSetHelper.pas',
  DAO.Pessoas in 'Model\DAO.Pessoas.pas',
  DAO.Enderecos in 'Model\DAO.Enderecos.pas',
